        <activity android:name=".ApplicantViewResidence" />
        <activity android:name=".ResidenceDetails" />
        <activity android:name=".ResidenceList" />
        <activity android:name=".AddResidence" />
        <activity android:name=".HousingOfficerMenu" />
        <activity android:name=".ApplicantMenu" />
        <activity android:name=".SignUp" />
        <activity android:name=".Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>